{
  "presets": [
    "react",
    ["env", {
      "modules": false,
      "useBuiltIns": "entry",
      "targets": {
        "browsers": [
          'Chrome >= 60',
          'Safari >= 10.1',
           'Firefox >= 54',
           'Edge >= 15',
           'ie >= 11'
         ]
      },
      "debug": true
    }]
  ],
  "plugins": [
    "transform-object-rest-spread"
  ]
}
