{
  "env": {
    "build": {
      "presets": [
        "react",
        ["env", {
          "modules": false,
          "useBuiltIns": "entry",
          "targets": {
            "browsers": [
              'Chrome >= 60',
              'Safari >= 10.1',
              'Firefox >= 54',
              'Edge >= 15',
              'ie >= 11'
            ]
          }
        }]
      ],
      "plugins": [
        ["transform-object-rest-spread", {"useBuiltIns": true}]
      ] 
    },
    "test": {
    "presets": [
      "react",
      ["env", {
        "modules": "commonjs",
        "useBuiltIns": "entry",
        "targets": {
          "browsers": [
            'Chrome >= 60',
            'Safari >= 10.1',
            'Firefox >= 54',
            'Edge >= 15',
            'ie >= 11'
          ]
        }
      }]
      ],
      "plugins": [
        ["transform-object-rest-spread", {"useBuiltIns": true}]
      ]
    }
  }
}
