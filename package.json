{
  "name": "hmda-platform-ui",
  "version": "0.0.0",
  "description": "Frontend code for the HMDA filing system",
  "main": "index.js",
  "engines": {
    "node": ">=6.x"
  },
  "scripts": {
    "test": "jest --coverage",
    "snyk": "snyk test",
    "clean": "rimraf ./dist && mkdirp ./dist ./dist/js ./dist/css ./dist/fonts ./dist/img",
    "js": "rimraf ./dist/js/app.min.js.map && browserify -d -t [ babelify --presets [ es2015 react ] --plugins [ babel-plugin-transform-object-rest-spread ] ] -p [ minifyify --map app.min.js.map --output ./dist/js/app.min.js.map --uglify [ -cm ] ] ./src/js/index.js > ./dist/js/app.min.js",
    "renew": "browserify -d -t [ babelify --presets [ es2015 react ] --plugins [ babel-plugin-transform-object-rest-spread ] ] -p [ minifyify --map silent_renew.js.map --output ./dist/js/silent_renew.js.map --uglify [ -cm ] ] ./src/js/silent_renew.js > ./dist/js/silent_renew.js",
    "sass": "node-sass --quiet --output-style=compressed --source-map-embed --source-map=./dist/css/ ./src/sass/app.scss ./dist/css/app.min.css && cp node_modules/uswds/src/fonts/sourcesanspro-* ./dist/fonts",
    "html": "html-minifier --collapse-whitespace ./src/index.html -o ./dist/index.html && html-minifier --collapse-whitespace ./src/silent_renew.html -o ./dist/silent_renew.html",
    "img": "imagemin ./src/img/* -o ./dist/img && cp -r ./node_modules/uswds/dist/img ./dist",
    "env": "./env.sh",
    "postinstall": "npm run clean && npm run build && npm run renew",
    "watch:js": "nodemon --exitcrash -e js,jsx -w src/js -i src/js/silent_renew.js -x 'npm run js'",
    "watch:renew": "nodemon --exitcrash -w src/js/silent_renew.js -x 'npm run renew'",
    "watch:tests": "nodemon --exitcrash -e js -w __tests__ -x 'jest --coverage -o'",
    "watch:sass": "nodemon --exitcrash -e scss -w src/sass -x 'npm run sass'",
    "watch:html": "nodemon --exitcrash -e html -w src -x 'npm run html'",
    "watch": "npm run watch:js & npm run watch:renew & npm run watch:tests & npm run watch:sass & npm run watch:html",
    "build": "npm run js && npm run sass && npm run html && npm run img && npm run env"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cfpb/hmda-platform-ui.git"
  },
  "keywords": [
    "HMDA"
  ],
  "author": "Consumer Financial Protection Bureau",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/cfpb/hmda-platform-ui/issues"
  },
  "homepage": "https://github.com/cfpb/hmda-platform-ui#readme",
  "devDependencies": {
    "babel-jest": "17.0.2",
    "babel-plugin-transform-object-rest-spread": "6.19.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babelify": "7.3.0",
    "browserify": "13.1.1",
    "html-minifier": "3.1.1",
    "imagemin": "5.2.2",
    "imagemin-cli": "3.0.0",
    "jest-cli": "18.1.0",
    "minifyify": "7.3.4",
    "mkdirp": "0.5.1",
    "node-sass": "3.13.0",
    "nodemon": "1.11.0",
    "react-addons-test-utils": "15.4.0",
    "react-dom": "15.4.0",
    "react-router-scroll": "0.4.1",
    "reactify": "1.1.1",
    "redux-mock-store": "1.2.1",
    "rimraf": "2.5.4",
    "snyk": "1.19.1",
    "uglify-js": "2.7.4"
  },
  "browserify": {
    "transform": [
      "reactify",
      "babelify"
    ]
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/Wrapper.js"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/lodash",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/redux-mock-store",
      "<rootDir>/node_modules/redux-thunk",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/__tests__/Wrapper.js"
    ]
  },
  "dependencies": {
    "file-saver": "1.3.3",
    "immutable": "3.8.1",
    "moment": "2.17.0",
    "oidc-client": "1.2.0",
    "react": "15.4.0",
    "react-dropzone": "3.10.0",
    "react-redux": "4.4.6",
    "react-router": "2.6.0",
    "react-router-redux": "4.0.7",
    "react-select": "0.9.1",
    "redux": "3.6.0",
    "redux-logger": "2.7.4",
    "redux-oidc": "2.2.1",
    "redux-thunk": "2.1.0",
    "uswds": "0.14.0"
  }
}
